{
  "manifest_version": 2,
  "name": "Form Data",
  "description": "Looks at some form data.",
  "version": "0.9",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["webRequest", "storage", "<all_urls>"]
}
